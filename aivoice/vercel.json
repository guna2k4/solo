{
  "version": 2,
  "cleanUrls": true,
  "routes": [
    {
      "src": "/styles.css",
      "headers": {
        "Content-Type": "text/css",
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/styles.css"
    },
    {
      "src": "/(.*)",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/index.html"
    }
  ]
}
